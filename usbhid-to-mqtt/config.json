{
    "name": "USB Hid to Mqtt",
    "version": "dev",
    "slug": "usbhid-to-mqtt",
    "description": "Submits token entered via usb hid to mqtt topic",
    "url": "https://github.com/gregod/addon-usbhid-to-mqtt",
    "startup": "application",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "boot": "auto",
    "hassio_api": true,
    "hassio_role": "default",
    "schema":  {
        "device" : "str",
        "mqtt_topic" : "str"
    },
    "options" : {
        "device" : "/dev/input/by-id/usb-HXGCoLtd_27db-event-kbd",
        "mqtt_topic" : "ggtest/tokens"
    },
    "image": "gregod/{arch}-addon-usbhid-to-mqtt"
  }
